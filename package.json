{
  "name": "@moln/dependency-container",
  "description": "A lightweight dependency injection container for TypeScript/JavaScript for constructor injection.",
  "author": "moln.xie@gmail.com",
  "version": "1.0.3",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "test:coverage": "tsdx test --coverage",
    "lint": "tsdx lint",
    "lint:fix": "tsdx lint --fix",
    "prepare": "tsdx build"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/dependency-container.esm.js",
  "devDependencies": {
    "coveralls": "^3.1.0",
    "husky": "^4.2.5",
    "lodash": "^4.17.15",
    "reflect-metadata": "^0.1.13",
    "tsdx": "^0.13.2",
    "tslib": "^1.11.2",
    "typescript": "^3.8.3"
  },
  "keywords": [
    "di",
    "ioc",
    "dependency",
    "injection",
    "container",
    "inversion",
    "decorators",
    "abstract factory"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Moln/dependency-container.git"
  },
  "homepage": "https://github.com/Moln/dependency-container",
  "bugs": {
    "url": "https://github.com/Moln/dependency-container/issues"
  }
}
